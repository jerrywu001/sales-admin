{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "scripts": {
    "build:small": "run-p build:saas-small",
    "build:prod": "run-p build:saas-prod",
    "build:test": "run-p build:saas-test",
    "build:uat": "run-p build:saas-uat",
    "build:mock": "run-p build:saas-mock",
    "build:local": "run-p build:saas-local",
    "build-vite:prod": "run-p build-vite:saas-prod",
    "build-vite:test": "run-p build-vite:saas-test",
    "build-vite:uat": "run-p build-vite:saas-uat",
    "build-vite:mock": "run-p build-vite:saas-mock",
    "build:login-small": "pnpm -F @app/login run build:small",
    "build:login-prod": "pnpm -F @app/login run build:prod",
    "build:login-test": "pnpm -F @app/login run build:test",
    "build:login-uat": "pnpm -F @app/login run build:uat",
    "build:login-mock": "pnpm -F @app/login run build:mock",
    "build:login-local": "pnpm -F @app/login run build:local",
    "build-vite:login-prod": "pnpm -F @app/login run build-vite:prod",
    "build-vite:login-test": "pnpm -F @app/login run build-vite:test",
    "build-vite:login-uat": "pnpm -F @app/login run build-vite:uat",
    "build-vite:login-mock": "pnpm -F @app/login run build-vite:mock",
    "build:saas-small": "pnpm -F @app/main run build:small",
    "build:saas-prod": "pnpm -F @app/main run build:prod",
    "build:saas-test": "pnpm -F @app/main run build:test",
    "build:saas-uat": "pnpm -F @app/main run build:uat",
    "build:saas-mock": "pnpm -F @app/main run build:mock",
    "build:saas-local": "pnpm -F @app/main run build:local",
    "build-vite:saas-prod": "pnpm -F @app/main run build-vite:prod",
    "build-vite:saas-test": "pnpm -F @app/main run build-vite:test",
    "build-vite:saas-uat": "pnpm -F @app/main run build-vite:uat",
    "build-vite:saas-mock": "pnpm -F @app/main run build-vite:mock",
    "dev": "run-p dev:saas dev:login",
    "dev:mock": "run-p dev:saas-mock dev:login-mock",
    "dev:docs": "pnpm -F docs run dev",
    "dev:saas": "pnpm -F @app/main run dev",
    "dev:saas-mock": "pnpm -F @app/main run dev:mock",
    "dev:login": "pnpm -F @app/login run dev",
    "dev:login-mock": "pnpm -F @app/login run dev:mock",
    "build:docs": "pnpm -F docs run build",
    "preview": "npx serve dist",
    "preview:docs": "pnpm -F docs run preview --host",
    "prepare": "husky",
    "stylelint": "run-p stylelint:saas stylelint:login",
    "stylelint:saas": "pnpm -F @app/main run stylelint",
    "stylelint:login": "pnpm -F @app/login run stylelint",
    "lint": "npm run oxc && npm run eslint",
    "oxc": "oxlint",
    "eslint": "eslint \"**/*.{ts,tsx,vue}\"",
    "eslint:inspector": "npx @eslint/config-inspector",
    "typecheck": "run-p typecheck:saas typecheck:login",
    "typecheck:login": "pnpm -F @app/login run typecheck",
    "typecheck:saas": "pnpm -F @app/main run typecheck",
    "test": "pnpm -F @app/main run test",
    "clean": "node clean.cjs"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "nprogress": "^0.2.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "dayjs": "^1.11.13",
    "eslint-plugin-vue": "^10.3.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "oxlint": "^1.8.0",
    "rollup-copy-plugin": "^0.1.0",
    "serve": "^14.2.4",
    "shelljs": "^0.10.0",
    "typescript": "5.6.3",
    "vite": "^7.0.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "pnpm": {
    "patchedDependencies": {
      "vite-plugin-checker": "patches/vite-plugin-checker.patch"
    }
  }
}
